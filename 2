#include <iostream>

int main() {
    const int N = 5;
    int arr1[N] = {1, 2, 3, 4, 5};
    int arr2[N] = {6, 7, 8, 9, 10};
    int arr3[N];

    for (int i = 0; i < N; ++i) {
        arr3[i] = arr1[i] + arr2[i];
    }

    for (int i = 0; i < N; ++i) {
        std::cout << arr3[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
