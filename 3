#include <iostream>

int main() {
    const int N = 10; 
    int arr[N] = {1, 0, 3, 0, 5, 0, 7, 0, 9, 0};

    int j = 0;
    for (int i = 0; i < N; ++i) {
        if (arr[i] != 0) {
            arr[j++] = arr[i];
        }
    }

    while (j < N) {
        arr[j++] = -1;
    }

    for (int i = 0; i < N; ++i) {
        std::cout << arr[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
